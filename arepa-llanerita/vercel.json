{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images|fonts)/(.*)",
      "dest": "public/$1/$2"
    },
    {
      "src": "/manifest.json",
      "dest": "public/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "public/sw.js"
    },
    {
      "src": "/.well-known/(.*)",
      "dest": "public/.well-known/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie",
    "CACHE_DRIVER": "file"
  },
  "functions": {
    "public/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  }
}
